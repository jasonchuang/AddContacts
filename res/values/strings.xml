<!--
    The string named "app_name" defines the application's visible name.
    The string array "Planets" defines an array of 9 strings. The application loads
    this array into the spinner's array adapter.
    The string "planet_prompt" defines the prompt for the result text box.
 -->
<resources>
    <string name="app_name">Add Contacts</string>
    <string name="add_description">Please input the ontacts number you want to add in the Contact counts field and press add menu button to start adding contacts.</string>
    <string name="contact_counts">Contact counts:</string>
    <string name="contact_counts_hint">Maximum 1000</string>
    <string name="menu_add">Add</string>
    <string name="adding_contacts">Adding contacts...</string>
    <string name="adding_contacts_success">Adding contacts successfully</string>
    <string name="add_failed_max_count">Add failed, exceeding Max add counts!!</string>
    <string name="add_failed_invalid_format">Add failed, inavlid number format!!</string>
</resources>
